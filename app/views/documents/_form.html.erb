<%= simple_form_for [@vehicle, @maintenance, @document], url: vehicle_maintenance_documents_path, :html => {:multipart => true} do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.file_field :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
